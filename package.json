  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@sissel/eslint-config": "^1.2.1",
    "@sissel/prettier-config": "^1.2.5",
    "@sissel/tsconfig": "^1.0.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@11ty/eleventy": "^2.0.1"
