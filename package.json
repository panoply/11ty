{
  "name": "11ty",
  "version": "0.2.0",
  "description": "An Eleventy wrapper for type supported configuration.",
  "keywords": [
    "11ty",
    "eleventy",
    "types",
    "declarations",
    "wrapper"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/panoply/11ty"
  },
  "bugs": {
    "url": "https://github.com/panoply/11ty/issues"
  },
  "type": "module",
  "types": "./index.d.ts",
  "module": "./index.mjs",
  "main": "./index.cjs",
  "exports": {
    "require": "./index.cjs",
    "import": "./index.mjs"
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "postversion": "pnpm bump",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "prettier": "@sissel/prettier-config",
  "eslintConfig": {
    "extends": "@sissel/eslint-config",
    "ignorePatterns": [
      "dist"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@sissel/eslint-config": "^1.0.0",
    "@sissel/prettier-config": "^1.0.0",
    "@sissel/tsconfig": "^1.0.0",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "@11ty/eleventy": "^1.0.1"
  },
  "dependencies": {
    "@types/browser-sync": "^2.26.3"
  }
}
